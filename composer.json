{
    "name": "jiny/ui2",
    "description": "jiny theme ui hpyer with bootstrap",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3|^8.0",
        "livewire/livewire": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\UI2\\":"src/"
        },
        "files" : [
            "src/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\UI2\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\UI2\\JinyUI2ServiceProvider"
            ]
        }
    }
}
